{
  "name": "optimizely-cms-sdk",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./react": {
      "import": "./dist/esm/render/react.js",
      "require": "./dist/cjs/render/react.js"
    },
    "./graph": {
      "import": "./dist/esm/graph/index.js",
      "require": "./dist/cjs/graph/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json",
    "dev": "pnpm run build --watch",
    "test": "vitest --typecheck"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.7.0",
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@types/react": "^19",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  }
}
